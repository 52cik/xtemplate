    "gulp": "^3.8.7",
    "gulp-clean": "^0.3.1",
    "gulp-filter": "^1.0.0",
    "gulp-footer": "^1.0.5",
    "gulp-header": "^1.0.5",
    "gulp-jscs": "^1.1.0",
